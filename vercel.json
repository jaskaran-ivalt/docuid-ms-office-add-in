{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": null,
  "rewrites": [
    {
      "source": "/",
      "destination": "/taskpane.html"
    },
    {
      "source": "/index.html",
      "destination": "/taskpane.html"
    },
    {
      "source": "/api/biometric/:path*",
      "destination": "https://dev.docuid.net/api/biometric/:path*"
    },
    {
      "source": "/api/documents/:id/download",
      "destination": "https://dev.docuid.net/api/documents/:id/download"
    },
    {
      "source": "/api/documents/:id/content",
      "destination": "https://dev.docuid.net/api/documents/:id/content"
    },
    {
      "source": "/api/dashboard/:path*",
      "destination": "https://dev.docuid.net/api/dashboard/:path*"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET, POST, PUT, DELETE, OPTIONS"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "Content-Type, Authorization, x-api-key"
        }
      ]
    }
  ]
}
